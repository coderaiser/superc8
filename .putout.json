{
    "rules": {
        "gitignore/add": "off",
        "gitignore/sort": "off",
        "typescript/convert-generic-to-shorthand": "off",
        "nodejs/add-missing-strict-mode": "off",
        "package-json/add-type": "off",
        "package-json/apply-https-to-repository-url": "off",
        "convert-index-of-to-includes": "off",
        "nodejs/declare": "off",
        "nodejs/add-node-prefix": "off",
        "nodejs/group-require-by-id": "off",
        "conditions/apply-consistent-blocks": "off",
        "variables/remove-unused": "off",
        "apply-shorthand-properties": "off",
        "destructuring/apply-object": "off",
        "maybe/array": "off",
        "remove-console": "off",
        "convert-to-arrow-function": "off",
        "convert-template-to-string": "off",
        "regexp/apply-starts-with": "off",
        "types/convert-typeof-to-is-type": "off",
        "conditions/merge-if-statements": "off",
        "conditions/remove-useless-else": "off",
        "for-of/for-each": "off",
        "apply-at": "off",
        "destructuring/apply-array": "off",
        "destructuring/convert-object-to-array": "off",
        "variables/remove-useless-declarations": "off",
        "destructuring/merge-properties": "off",
        "convert-quotes-to-backticks": "off",
        "promises/remove-useless-async": "off",
        "regexp/optimize": "off",
        "tape/jest": "off",
        "nodejs/declare-after-require": "off",
        "conditions/evaluate": "off",
        "remove-unused-expressions": "off",
        "apply-arrow": "off",
        "new/remove-useless": "off",
        "remove-unreachable-code": "off",
        "remove-nested-blocks": "off",
        "simplify-ternary/value": "off",
        "variables/remove-useless": "off",
        "conditions/apply-equal": "off",
        "conditions/remove-zero": "off",
        "apply-dot-notation": "off"
    },
    "ignore": ["tmp", "fixtures"]
}
